  <Form.Group controlId='email'>\n                                    <Form.Label>Email Address</Form.Label>\n                                    <Form.Control\n                                        type='email'\n                                        placeholder='Enter Email'\n                                        value={email}\n                                        onChange={(e) => setEmail(e.target.value)}\n                                        disabled\n                                    >\n                                    </Form.Control>\n                                </Form.Group>\n                                <Form.Group controlId='verified'>\n                                    <Form.Check\n                                        type='checkbox'\n                                        label='Is verified'\n                                        checked={verified}\n                                        onChange={handleVerified}\n                                    >\n                                    </Form.Check>\n                                </Form.Group>\n\n                                <Form.Group controlId='isadmin'>\n                                    <Form.Check\n                                        type='checkbox'\n                                        label='Is Admin'\n                                        checked={isAdmin}\n                                        onChange={handleAdmin}\n                                    >\n                                    </Form.Check>\n                                </Form.Group>\n\n                                <Button type='submit' variant='primary'>\n                                    Update\n                                </Button>\n\n                            </Form>\n                        </Col>\n                    </Row>\n\n                    </>}\n\n\n\n        </div>\n\n    )\n}\n\nexport default UserEditScreen","/home/aziz/Desktop/store/client/src/screen