         <ListGroup.Item>\n                                <Row>\n                                    <Col>Items:</Col>\n                                    <Col>&#2547; {cart.itemsPrice}</Col>\n                                </Row>\n                            </ListGroup.Item>\n\n                            {/* <ListGroup.Item>\n                                <Row>\n                                    <Col>Shipping:</Col>\n                                    <Col>&#2547; {cart.shippingPrice}</Col>\n                                </Row>\n                            </ListGroup.Item> */}\n\n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>Tax:</Col>\n                                    <Col>&#2547; {cart.taxPrice}</Col>\n                                </Row>\n                            </ListGroup.Item>\n\n\n            \n\n                            \n                            <ListGroup.Item>\n                                <Row>\n                                    <Col>Total:</Col>\n                                    <Col>&#2547; {cart.totalPrice}</Col>\n                                </Row>\n                            </ListGroup.Item>\n\n\n                            <ListGroup.Item>\n                                {error && <Message variant='danger'>{error}</Message>}\n                            </ListGroup.Item>\n\n                            <ListGroup.Item>\n                                <Button\n                                    type='button'\n                                    className='btn-block'\n                                    disabled={cart.cartItems === 0}\n                                    onClick={placeOrder}\n                                    size=\"sm\"\n                                >\n                                    Place Order\n                                </Button>\n                            </ListGroup.Item>\n\n               